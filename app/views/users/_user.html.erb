<div id="<%= dom_id user %>">
  <p>
    <strong>Login:</strong>
    <%= user.username %>
  </p>

  <p>
    <strong>Hasło:</strong>
    <%= user.password_digest %>
  </p>

  <p>
    <strong>Imię:</strong>
    <%= user.first_name %>
  </p>

  <p>
    <strong>Nazwisko:</strong>
    <%= user.last_name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>


</div>
